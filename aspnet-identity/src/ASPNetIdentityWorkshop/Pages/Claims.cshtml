@page
@model ClaimsModel
@{
    ViewData["Title"] = "Claims";
}

<h1>@ViewData["Title"]</h1>

<table class="table-bordered">
    <thead>
        <tr>
           <th>Type</th>
           <th>Value Type</th>
           <th>Value</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var claim in User.Claims)
        {
            <tr>
                <td>@claim.Type</td>
                <td>@claim.ValueType</td>
                <td>@claim.Value</td>
            </tr>
        }
    </tbody>
</table>